{
  "manifest_version": 3,
  "name": "Private Equity Info Generator",
  "version": "1.0.0",
  "description": "A Chrome extension to gather private equity info using ChatGPT.",
  "permissions": ["activeTab", "storage", "scripting"],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  }
}
